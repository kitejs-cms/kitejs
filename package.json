{
  "name": "kitejs",
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "clean": "find . -name 'node_modules' -type d -exec rm -rf {} +"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.0.8",
    "prettier": "^3.5.2",
    "tailwindcss": "^4.0.8",
    "tsx": "^4.19.3",
    "turbo": "^2.4.2",
    "typescript": "5.7.3"
  },
  "packageManager": "pnpm@10.11.0",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "inquirer": "^12.5.2"
  }
}

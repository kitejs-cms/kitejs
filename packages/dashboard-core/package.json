{
  "name": "@kitejs-cms/dashboard-core",
  "version": "0.1.0-alpha.4",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles.css": "./dist/index.css",
    "./*": {
      "import": "./dist/*.js",
      "require": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "scripts": {
    "build:ts": "tsc --build tsconfig.json",
    "build:css": "tailwindcss -i ./src/styles.css -o ./dist/index.css",
    "build": "pnpm run build:ts && pnpm run build:css",
    "dev": "pnpm run dev:ts & pnpm run dev:css",
    "dev:ts": "tsc --build tsconfig.json --watch",
    "dev:css": "tailwindcss -i ./src/styles.css -o ./dist/index.css --watch",
    "check-types": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx --max-warnings 0"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@kitejs-cms/core": "workspace:*",
    "@kitejs-cms/eslint-config": "workspace:*",
    "@kitejs-cms/tailwind-config": "workspace:*",
    "@kitejs-cms/typescript-config": "workspace:*",
    "@types/node": "^22.13.0",
    "@types/react": "19.0.8",
    "@types/react-color": "^3.0.13",
    "@types/react-dom": "19.0.3",
    "eslint": "^9.20.0",
    "typescript": "5.7.3"
  },
  "dependencies": {
    "@aws-sdk/s3-request-presigner": "^3.787.0",
    "@blocknote/core": "^0.36.0",
    "@blocknote/mantine": "^0.36.0",
    "@blocknote/react": "^0.36.0",
    "@blocknote/xl-multi-column": "^0.36.0",
    "@calumk/editorjs-columns": "^0.3.2",
    "@hookform/resolvers": "^4.1.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.3.0",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-icons": "^1.3.2",
